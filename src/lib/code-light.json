{
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#111014",
    "editorHoverWidget.background": "#dfd9e4",
    "editorHoverWidget.foreground": "#1a181e",
    "editorHoverWidget.border": "#dfd9e4",
    "breadcrumb.background": "#f8f2fa",
    "breadcrumb.foreground": "#4c4855",
    "breadcrumb.focusForeground": "#232129",
    "breadcrumbPicker.background": "#dfd9e4",
    "breadcrumbPicker.foreground": "#1a181e",
    "breadcrumb.activeSelectionForeground": "#000000",
    "editorLineNumber.foreground": "#6e6979",
    "editorLineNumber.activeForeground": "#232129",
    "editor.lineHighlightBorder": "#dfd9e4",
    "editorIndentGuide.background": "#dfd9e4",
    "editorIndentGuide.activeBackground": "#827c8c",
    "activityBarBadge.background": "#fd7cc0",
    "sideBarTitle.foreground": "#4c4855",
    "sideBar.background": "#f8f2fa",
    "activityBar.background": "#f8f2fa",
    "titleBar.activeBackground": "#f8f2fa",
    "titleBar.activeForeground": "#232129",
    "statusBar.background": "#f8f2fa",
    "statusBar.foreground": "#2f2c36",
    "statusBarItem.remoteBackground": "#dfd9e4",
    "statusBarItem.remoteForeground": "#1a181e",
    "editorGroupHeader.tabsBackground": "#f8f2fa",
    "tab.activeBackground": "#f8f2fa",
    "tab.activeForeground": "#2f2c36",
    "tab.activeBorder": "#f8f2fa",
    "tab.activeBorderTop": "#4c4855",
    "tab.inactiveBackground": "#f8f2fa",
    "tab.inactiveForeground": "#4c4855",
    "tab.inactiveBorder": "#f8f2fa",
    "tab.border": "#f8f2fa",
    "tab.hoverBorder": "#ffffff"
  },
  "tokenColors": [
    {
      "scope": "string",
      "settings": {
        "foreground": "#0051a7"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#4c4855"
      }
    },
    {
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#3d3a45"
      }
    },
    {
      "scope": "constant.character.escape, text.html constant.character.entity.named",
      "settings": {
        "foreground": "#00111d"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#8f1614"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#00428a"
      }
    },
    {
      "scope": "variable, variable.parameter, support.variable, variable.language, support.constant, meta.definition.variable entity.name.function, meta.function-call.arguments",
      "settings": {
        "foreground": "#00111d"
      }
    },
    {
      "scope": "keyword.other",
      "settings": {
        "foreground": "#00428a"
      }
    },
    {
      "scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
      "settings": {
        "foreground": "#3d3a45"
      }
    },
    {
      "scope": "entity.name.function, support.function",
      "settings": {
        "foreground": "#1a181e"
      }
    },
    {
      "scope": "storage.type, storage.modifier, storage.control",
      "settings": {
        "foreground": "#6e6979"
      }
    },
    {
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#f8f2fa",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "support.type, constant.other.key",
      "settings": {
        "foreground": "#a74901"
      }
    },
    {
      "scope": "entity.name.type, entity.other.inherited-class, entity.other",
      "settings": {
        "foreground": "#111014",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#4c4855",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment punctuation.definition.comment, string.quoted.docstring",
      "settings": {
        "foreground": "#4c4855",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#4c4855"
      }
    },
    {
      "scope": "entity.name, entity.name.type.class, support.type, support.class, meta.use",
      "settings": {
        "foreground": "#a74901"
      }
    },
    {
      "scope": "variable.object.property, meta.field.declaration entity.name.function",
      "settings": {
        "foreground": "#111014"
      }
    },
    {
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#111014"
      }
    },
    {
      "scope": "meta.function entity.name.function",
      "settings": {
        "foreground": "#1a181e"
      }
    },
    {
      "scope": "template.expression.begin, template.expression.end",
      "settings": {
        "foreground": "#3d3a45"
      }
    },
    {
      "scope": "punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end",
      "settings": {
        "foreground": "#4c4855"
      }
    },
    {
      "scope": "meta.embedded, source.groovy.embedded, meta.template.expression",
      "settings": {
        "foreground": "#00111d"
      }
    },
    {
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#111014"
      }
    },
    {
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#111014"
      }
    },
    {
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#3d3a45"
      }
    },
    {
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#a74901"
      }
    },
    {
      "scope": "entity.other.attribute-name.id",
      "settings": {
