  "packageManager": "npm@11.7.0",
  "scripts": {
    "astro": "astro",
    "build": "astro check --minimumSeverity warning && astro build",
    "dev": "astro dev --host",
    "prepare": "patch-package && npm --workspaces run check",
    "preview": "astro preview",
    "pw:install": "playwright install --with-deps chromium --only-shell",
    "pw:open": "playwright test --ui-host=0.0.0.0",
    "pw:run": "playwright test",
    "update:latest": "npm-check-updates --interactive --workspaces --filter \"arcjet, @arcjet/*, nosecone, @nosecone/*\"",
    "update:arcjet": "npm up arcjet \"@arcjet/*\" nosecone \"@nosecone/*\" --workspaces",
    "update:minor": "npm-check-updates --interactive --cooldown 30 --target minor --workspaces",
    "update:major": "npm-check-updates --interactive --cooldown 30 --target @latest --workspaces"
  },
  "type": "module",
  "version": "0.0.1",
  "workspaces": [
    "snippets/*"
  ]
